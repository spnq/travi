{
	"name": "travi",
	"displayName": "travi",
	"description": "Cyrillic Lorem Ipsum",
	"version": "1.0.1",
	"publisher": "spnq",
	"icon": "tea.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/spnq/travi"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"keywords": [
		"lorem",
		"ipsum",
		"cyrillic",
		"text",
		"generator"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.travi",
				"title": "Travi Cyrillic Text Generator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "jest --watch --verbose --colors",
		"cover": "jest --coverage"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/node": "^7.7.4",
		"@babel/preset-env": "^7.7.6",
		"@types/glob": "^7.1.1",
		"@types/jest": "^24.0.23",
		"@types/node": "^10.12.21",
		"glob": "^7.1.6",
		"jest": "^24.9.0",
		"ts-jest": "^24.2.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"vscode-test": "^1.2.3"
	}
}
