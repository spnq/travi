{
	"name": "travi",
	"displayName": "travi",
	"description": "Cyrillic Lorem ipsum",
	"version": "0.0.1",
	"publisher": "spnq",
	"icon": "tea.png",
	"repository": {"type": "git", "url": "https://github.com/spnq/travi"},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Snippets"
	],
		"activationEvents": [
			"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.travi",
				"title": "Travi Cyrillic Text Generator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
